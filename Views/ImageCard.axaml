<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:photocool.Views"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="photocool.Views.ImageCard">
    <StackPanel Orientation="Vertical" Margin="10">
        <Image HorizontalAlignment="Center" Name="ImagePreview" Width="125" Height="125"></Image>
        <Label HorizontalAlignment="Center" Name="ImageName"></Label>
    </StackPanel>
</UserControl>
